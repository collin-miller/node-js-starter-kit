{
  "name": "js-starter-kit",
  "version": "1.0.0",
  "description": "This project is created as a template for node.js web apps",
  "main": "index.js",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-loader": "^8.0.6",
    "babel-node": "0.0.1-security",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "commitizen": "^4.0.3",
    "commitlint": "^8.1.0",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.0.5",
    "nodemon": "^1.19.2",
    "standard-version": "^7.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx webpack",
    "start:dev": "npx nodemon index.js --watch ./**.js --exec babel-node",
    "commit": "npx git-cz",
    "first-release": "npm run release -- --first-release",
    "release": "npx standard-version --preset angular"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/collin-miller/node-js-starter-kit.git"
  },
  "author": "Collin Miller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/collin-miller/node-js-starter-kit/issues"
  },
  "homepage": "https://github.com/collin-miller/node-js-starter-kit#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}